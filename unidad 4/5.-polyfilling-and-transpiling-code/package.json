{
    "type": "module",
    "name": "Polyfilling and transpiling",
    "version": "1.0.0",
    "description": "Code to show how to polyfill and transpile code",
    "main": "src/index.js",
    "scripts": {
        "build:modern": "webpack --config webpack.modern.js --mode $modo",
        "build:legacy": "webpack --config webpack.legacy.js --mode $modo",
        "des": "cross-env modo=development npm-run-all --serial build:modern build:legacy",
        "prod": "cross-env modo=production npm-run-all --serial build:modern build:legacy",
        "limpia": "rimraf dist"
      },
    "browserslist": "> 0.25%, not dead",
    "keywords": [
        "webpack",
        "polyfill",
        "transpile"
      ],
